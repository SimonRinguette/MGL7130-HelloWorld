<?xml version='1.0' encoding='utf-8'?>
<widget id="io.ionic.starter" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HW MGL7130 Ionic</name>
    <description>Example hello world de ionic-3.</description>
    <author email="sringuette@trisotech.com">Simon Ringuette</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/icon/ldpi.png" />
        <icon density="mdpi" src="resources/android/icon/mdpi.png" />
        <icon density="hdpi" src="resources/android/icon/hdpi.png" />
        <icon density="xhdpi" src="resources/android/icon/xhdpi.png" />
        <icon density="xxhdpi" src="resources/android/icon/xxhdpi.png" />
        <splash density="land-ldpi" src="resources/android/splash/screen-land-ldpi.png" />
        <splash density="land-mdpi" src="resources/android/splash/screen-land-mdpi.png" />
        <splash density="land-hdpi" src="resources/android/splash/screen-land-hdpi.png" />
        <splash density="land-xhdpi" src="resources/android/splash/screen-land-xhdpi.png" />
        <splash density="land-xxhdpi" src="resources/android/splash/screen-land-xxhdpi.png" />
        <splash density="port-ldpi" src="resources/android/splash/screen-port-ldpi.png" />
        <splash density="port-mdpi" src="resources/android/splash/screen-port-mdpi.png" />
        <splash density="port-hdpi" src="resources/android/splash/screen-port-hdpi.png" />
        <splash density="port-xhdpi" src="resources/android/splash/screen-port-xhdpi.png" />
        <splash density="port-xxhdpi" src="resources/android/splash/screen-port-xxhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="resources/ios/icon/icon-180.png" width="180" />
        <icon height="60" src="resources/ios/icon/icon-60.png" width="60" />
        <icon height="120" src="resources/ios/icon/icon-120.png" width="120" />
        <icon height="76" src="resources/ios/icon/icon-76.png" width="76" />
        <icon height="152" src="resources/ios/icon/icon-152.png" width="152" />
        <icon height="40" src="resources/ios/icon/icon-40.png" width="40" />
        <icon height="80" src="resources/ios/icon/icon-80.png" width="80" />
        <icon height="57" src="resources/ios/icon/icon-57.png" width="57" />
        <icon height="114" src="resources/ios/icon/icon-114.png" width="114" />
        <icon height="72" src="resources/ios/icon/icon-72.png" width="72" />
        <icon height="144" src="resources/ios/icon/icon-144.png" width="144" />
        <icon height="29" src="resources/ios/icon/icon-29.png" width="29" />
        <icon height="58" src="resources/ios/icon/icon-58.png" width="58" />
        <icon height="50" src="resources/ios/icon/icon-50.png" width="50" />
        <icon height="100" src="resources/ios/icon/icon-100.png" width="100" />
        <splash height="480" src="resources/ios/splash/splash-320-480.png" width="320" />
        <splash height="960" src="resources/ios/splash/splash-640-960.png" width="640" />
        <splash height="1024" src="resources/ios/splash/splash-768-1024.png" width="768" />
        <splash height="2048" src="resources/ios/splash/splash-1536-2048.png" width="1536" />
        <splash height="768" src="resources/ios/splash/splash-1024-768.png" width="1024" />
        <splash height="1536" src="resources/ios/splash/splash-2048-1536.png" width="2048" />
        <splash height="1136" src="resources/ios/splash/splash-640-1136.png" width="640" />
        <splash height="1334" src="resources/ios/splash/splash-750-1334.png" width="750" />
        <splash height="2208" src="resources/ios/splash/splash-1242-2208.png" width="1242" />
        <splash height="1242" src="resources/ios/splash/splash-2208-1242.png" width="2208" />
    </platform>
    <plugin name="ionic-plugin-keyboard" spec="~2.2.1" />
    <plugin name="cordova-plugin-whitelist" spec="1.3.1" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.1" />
    <plugin name="cordova-plugin-ionic-webview" spec="^1.1.11" />
    <plugin name="cordova-plugin-device" spec="^1.1.4" />
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1">
        <variable name="GEOLOCATION_USAGE_DESCRIPTION" value="Pour une demo" />
    </plugin>
    <engine name="browser" spec="5.0.3" />
    <engine name="android" spec="7.0.0" />
    <engine name="ios" spec="4.5.4" />
</widget>
